pipeline {
    agent any

    stages{
        stage('First stage'){
            steps{
                sh 'echo My stage ONE'
            }
        }

        stage('Second Stage'){
            steps{
                sh 'echo "My stage TWO"'
            }
        }

        stage('Third stage'){
            steps {
                sh 'echo "My stage three"'
            }
        }


        stage('Four stage'){
            steps {
                sh 'echo "My stage four"'
            }
        }
    }
}